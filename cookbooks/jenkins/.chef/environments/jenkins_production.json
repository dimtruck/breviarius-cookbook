{
  "chef_type": "environment",
  "cookbook_versions": {
    "apt": "= 2.7.0",
    "aufs": "= 0.1.1",
    "auto-update": "= 0.3.0",
    "breviarius-jenkins": "= 0.0.1",
    "build-essential": "= 2.2.2",
    "chef-sugar": "= 3.1.0",
    "chef_handler": "= 1.1.6",
    "device-mapper": "= 0.1.0",
    "dmg": "= 2.2.2",
    "docker": "= 0.36.0",
    "dpkg_autostart": "= 0.1.12",
    "firewall": "= 1.0.1",
    "git": "= 4.1.0",
    "golang": "= 1.5.0",
    "homebrew": "= 1.12.0",
    "iptables": "= 1.0.0",
    "iptables-ng": "= 2.2.5",
    "jenkins": "= 2.2.2",
    "lxc": "= 2.0.0",
    "modules": "= 0.2.0",
    "ohai": "= 2.0.1",
    "openssh": "= 1.3.4",
    "poise": "= 1.0.12",
    "polipo": "= 0.1.0",
    "runit": "= 1.5.18",
    "sudo": "= 2.7.1",
    "sysctl": "= 0.6.2",
    "users": "= 1.8.2",
    "windows": "= 1.36.6",
    "yum": "= 3.5.3",
    "yum-epel": "= 0.6.0"
  },
  "default_attributes": {},
  "description": "Environment used for installing jenkins",
  "json_class": "Chef::Environment",
  "name": "jenkins_production",
  "override_attributes": {
    "apt": {
      "compile_time_update": true
    },
    "authorization": {
      "sudo": {
        "groups": [
          "admin",
          "wheel",
          "sysadmin"
        ],
        "include_sudoers_d": "true",
        "passwordless": "true"
      }
    },
    "breviarius_jenkins": {
      "git_repository": "git@github.com:dimtruck/breviarius-cookbook.git",
      "git_revision": "master"
    },
    "build-essential": {
      "compile_time": "true"
    },
    "databag_encrypted": true,
    "docker": {
      "version": "1.5.1"
    },
    "java": {
      "jdk_version": "7"
    },
    "jenkins": {
      "executor": {
        "timeout": 300
      },
      "ghprb": {
        "admin_list": "dimtruck breviarius-bot",
        "published_url": "http://ci.breviari.us/"
      },
      "home": "/home/jenkins/",
      "username": "breviarius-bot"
    },
    "ntp": {
      "servers": [
        "time.rackspace.com"
      ]
    },
    "openssh": {
      "server": {
        "match": {},
        "password_authentication": "no",
        "permit_root_login": "no",
        "port": [
          22
        ],
        "use_dns": "no",
        "x11_forwarding": "no"
      }
    },
    "security": {
      "create_groups": {
        "3001": "sysadmin"
      },
      "create_sudo": [
        "sysadmin"
      ],
      "delete_groups": {
        "3003": "nerds"
      },
      "delete_sudo": [
        "nerds"
      ],
      "firewall": {
        "allow_jumphosts": true,
        "allow_wideopen": true,
        "jumphosts": {
          "dfw1": "72.3.128.84",
          "hkg1": "120.136.34.22",
          "iad2": "69.20.0.1",
          "jenkins": "104.239.136.90",
          "lon1": "212.100.225.49",
          "lon3": "212.100.225.42",
          "ord1": "50.57.22.125",
          "syd2": "119.9.4.2"
        },
        "wideopen": {
          "public": "eth2"
        }
      }
    },
    "slaves": {
      "server": "104.239.149.114"
    }
  }
}
